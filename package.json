{
  "name": "seeds-campaign-explorer",
  "version": "0.5.0",
  "description": "A explorer for viewing the SEEDS Co-op Campaigns.",
  "main": "dist/app.js",
  "scripts": {
    "justrun": "node dist/app.js",
    "start": "tsc && node dist/app.js",
    "test": "cross-env NODE_ENV=test mocha -r ts-node/register test/**/*.test.ts",
    "clean": "npx rimraf dist",
    "clean:node": "npx rimraf node_modules",
    "dev": "npx tsc-watch --onSuccess \"nodemon\"",
    "knex:migrate:make": "knex --knexfile src/database/knexfile.ts migrate:make -x ts",
    "knex:migrate:latest": "knex --knexfile src/database/knexfile.ts migrate:latest",
    "knex:migrate:rollback": "knex --knexfile src/database/knexfile.ts migrate:rollback"
  },
