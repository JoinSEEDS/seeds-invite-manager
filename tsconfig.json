{
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "outDir": "dist",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "sourceMap": true,
        "strictNullChecks": false
      },
    "exclude": [
        "test",
        "dist/**"
    ],
    "lib": ["es2015"]
  }